gt-cache/
ics-importer-cache/
imagecache.*
pclzip-*.gz
wp-content/w3-.*
wp-content/w3tc.*
gallery/*
album/*

# wpengine specific
.smushit-status
_wpeprivate
wp-content/advanced-cache.php
wp-content/object-cache.php

# flywheel specific
wp-content/flywheel-config/

